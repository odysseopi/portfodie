/*
 *	responsive vimeo
 */
.vimeo
	max-width 940px
	padding 7%

.responsive-video-container
	position relative
	padding-bottom 40%
	padding-top 1.5625em
	height 0
	overflow hidden
	margin-bottom 60px

.responsive-video-container iframe,  
.responsive-video-container object,  
.responsive-video-container embed
	position absolute
	top 0
	left 0
	bottom 0em
	width 100%
	height 100%